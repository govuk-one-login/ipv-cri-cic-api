{
    "name": "cic",
    "version": "1.0.0",
    "description": "OneLogin CIC CRI",
    "repository": "https://github.com/govuk-one-login/ipv-cri-cic-api",
    "author": "GDS",
    "license": "MIT",
    "dependencies": {
        "@aws-lambda-powertools/logger": "1.5.1",
        "@aws-lambda-powertools/metrics": "1.5.1",
        "@aws-sdk/client-dynamodb": "3.984.0",
        "@aws-sdk/client-kms": "3.984.0",
        "@aws-sdk/client-s3": "3.984.0",
        "@aws-sdk/client-sqs": "3.984.0",
        "@aws-sdk/credential-providers": "3.984.0",
        "@aws-sdk/lib-dynamodb": "3.984.0",
        "@smithy/node-http-handler": "4.0.4",
        "ajv": "8.12.0",
        "aws-xray-sdk-core": "3.10.3",
        "axios": "1.13.5",
        "body-parser": "2.2.2",
        "class-validator": "0.14.1",
        "dotenv-cli": "7.4.4",
        "ecdsa-sig-formatter": "1.0.11",
        "esbuild": "0.25.2",
        "jose": "5.10.0",
        "node-jose": "2.2.0"
    },
    "scripts": {
        "unit": "./node_modules/.bin/jest --testPathPattern=tests/unit/ --coverage",
        "test:unit": "npm run compile && npm run unit",
        "lint:fix": "eslint --fix --output-file ./reports/eslint/report.html --format html -c eslint.config.mjs --ext *.ts .",
        "compile": "./node_modules/.bin/tsc",
        "lint": "eslint -c eslint.config.mjs *.ts .",
        "test:e2e": "JEST_JUNIT_OUTPUT_NAME=e2e-report.xml ./node_modules/.bin/jest --test-match '**/tests/api/e2e*.test.ts'",
        "test:infra": "./node_modules/.bin/jest --testMatch '**/infra/?(*.)test.ts' ",
        "api": "node --experimental-vm-modules ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/",
        "test:api": "npm run compile && npm run api",
        "test:pii": "bash ./check-logs.sh",
        "kill:dynamodblocal": "cd tests/contract; ./scripts/kill-dynamodb-local.sh",
        "start:dynamodblocal": "cd tests/contract; ./scripts/setup-dynamodb-local.sh",
        "test:pactconnection": "cd tests/contract; ./scripts/test-pact-connection.sh",
        "start:contract": "npm-run-all -s start:dynamodblocal test:pactconnection wait-start:contract",
        "wait-start:contract": "wait-on tcp:127.0.0.1:8000 && cd tests/contract && ts-node ./app.ts",
        "test:provider": "./node_modules/.bin/jest --testMatch '**/contract/CicCriProvider.test.ts' --coverage=false",
        "test:contract": "wait-on tcp:127.0.0.1:3000; cd tests/contract; npm run test:provider && npm run kill:dynamodblocal",
        "test:contract:ci": "dotenv -e.env.contract -- npm-run-all -p -r start:contract test:contract"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.148",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.14",
        "@types/js-yaml": "4.0.9",
        "@types/node": "20.14.8",
        "@types/node-jose": "1.1.13",
        "@typescript-eslint/eslint-plugin": "8.26.0",
        "aws-cdk-lib": "2.196.0",
        "dotenv": "17.2.2",
        "eslint": "9.21.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jest": "28.11.0",
        "eslint-plugin-jsdoc": "54.0.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-security": "1.6.0",
        "fast-xml-parser": "4.2.7",
        "jest": "29.7.0",
        "jest-html-reporter": "3.10.2",
        "jest-junit": "16.0.0",
        "jest-mock-extended": "3.0.7",
        "npm-run-all": "4.1.5",
        "ts-jest": "29.3.1",
        "ts-node": "10.9.2",
        "typescript": "4.9.5",
        "wait-on": "7.2.0",
        "yaml-cfn": "0.3.2",
        "aws4-axios": "3.4.0",
        "@pact-foundation/pact": "12.1.3",
        "express": "4.21.2",
        "express-asyncify": "2.1.2"
    },
    "engines": {
        "node": "22.*"
    },
      "overrides": {
    "form-data": ">=4.0.4"
    }
}
